{
  "name": "gfapi",
  "version": "0.1.1",
  "description": "Gameflip API",
  "keywords": ["Gameflip"],
  "homepage": "https://github.com/iJJi/gfapi",
  "bugs": { "url": "https://github.com/iJJi/gfapi/issues" },
  "author": "Eng-Shien Wu <engshien.wu@ijji.com>",
  "license": "MIT",
  "private": true,
  "files": ["index.js"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iJJi/gfapi.git"
  },
  "engines": { "node": ">=8.5.0" },
  "scripts": {
    "start": "node app/server.js",
    "build": "npm --prefix frontend install && npm --prefix frontend run build",
    "dev": "concurrently \"nodemon --watch app app/server.js\" \"npm --prefix frontend run dev\"",
    "bulk_listing": "node src/samples/bulk_listing.js",
    "docs": "jsdoc -c jsdoc_conf.js -d docs -P package.json index.js; (cd src/samples && docco -o ../../docs/samples *.js *.rb)"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "connect-redis": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.1",
    "file-type": "^16.5.4",
    "http-errors": "^1.8.1",
    "limiter": "^2.1.0",
    "redis": "^4.7.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "docco": "^0.9.1",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.1.7"
  },
  "main": "index.js",
  "directories": { "doc": "docs" },
  "type": "commonjs"
}
